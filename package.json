  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "npm run typeorm -- migration:create",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -d ./ormconfig.ts",
    "typeorm:migration:run": "npm run typeorm -- migration:run -d /ormconfig.ts",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert -d ./ormconfig.ts",
    "typeorm:database:drop": "npm run typeorm -- schema:drop -d ./ormconfig.ts",
    "typeorm:entity:create": "npm run typeorm -- entity:create",
    "typeorm:seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./ormconfig.ts",
    "typeorm:seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create -d ./ormconfig.ts"
  },
